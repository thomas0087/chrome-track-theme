{
    "manifest_version": 2,    
    "version": "0.1",
    "name": "rnp-sunset",
    "description": "A theme based on sunset in the Royal National Park.",
    "theme": {
        "images" : {
            "theme_frame" : "images/frame.png",
            "theme_toolbar" : "images/toolbar.png",
            "theme_ntp_background" : "images/background.png",
            "theme_tab_background" : "images/tab.png",
            "theme_ntp_attribution" : "images/attribution.png"
        },
        "colors" : {
            "ntp_link": [255,255,255],
            "ntp_text": [255,255,255],
            "ntp_section": [10,10,10],
            "ntp_section_link": [255,255,255],
            "ntp_section_text": [10 , 17 , 27],
            "ntp_background": [10 , 17 , 27],
            "frame": [10 , 17 , 27],
            "toolbar": [0 , 0 , 0],
            "tab_text": [255,255,255],
            "tab_background_text": [187,186,192],
            "bookmark_text": [255,255,255]
        },
        "tints" : {
            "buttons" : [-0.43, -0.5, -0.47]
        },
        "properties" : {
            "ntp_background_alignment" : "center",
            "ntp_background_repeat": "no-repeat",
            "ntp_logo_alternate" : 1
        }
    }
}
